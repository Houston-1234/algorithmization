class Hotel:
    def __init__(self, name):
        self.name = name
        self.rooms = {100:True, 101:True, 102:True, 103:True, 200:True, 201:True, 202:True, 203:True}
    def info_hotel(self):
        print(f"Название отеля - {self.name}\nСтатус комнат: ")
        for room, status in self.rooms.items():
            if status_text = :
                print("Свободный номер")
            else:
                print("Номер занят")
        print(f"Номер комнаты - {room}, статус - {status}")
    def room_reservation(self, room):
        if room in self.rooms:
            if room not in self.rooms:
                print(f"Комнаты {room} не существует")
            else:
                if self.rooms[room]==True:
                    print(f"Номер {room} успешно забронирован")
                else:
                    print(f"Номер {room} уже занят")
hotel = Hotel("Brawl Stars")
hotel.info_hotel()
hotel.room_reservation(100)
hotel.room_reservation(100)
hotel.room_reservation(10)
